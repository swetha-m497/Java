import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner A=new Scanner(System.in);
        int size=A.nextInt();
        int a[]=new int[size];
        for(int i=0;i<size;i++){
            a[i]=A.nextInt();
        }
        Arrays.sort(a);
        System.out.println("Youngest="+a[0]);
        System.out.println("Oldest="+a[size-1]);

        
    }
}
